<?xml version="1.0" encoding="UTF-8" ?>
<server>
  <!-- socks代理相关设置 -->
  <socksPort>2186</socksPort>
  <method>aes-256-cfb</method>
  <password>123456</password>

  <!-- 内网映射相关设置 -->
  <!-- 内网穿透代理策略：关闭：-1；全部转发至内网：0 (默认)；指定域名转发至内网 1。 -->
  <lanStrategy>0</lanStrategy>
  <lanServerPort>2187</lanServerPort>
  <!-- 内网映射域名，不指定的话默认"mynetylan.adolphor.com"，此域名下的所有请求，会转发到运行LAN服务的 "127.0.0.1" -->
  <lanHostName>mynetylan.adolphor.com</lanHostName>
</server>
